# Configuração para PostgreSQL Local - AWS Lightsail
# Mix Cotação Web - Ambiente de Produção

NODE_ENV=production
PORT=5000

# PostgreSQL Local (AWS Lightsail)
DATABASE_URL=postgresql://mixuser:sua_senha_aqui@localhost:5432/mixcotacao

# Configurações específicas do PostgreSQL
PGUSER=mixuser
PGPASSWORD=sua_senha_aqui
PGDATABASE=mixcotacao
PGHOST=localhost
PGPORT=5432

# Chave secreta para sessões (gere uma chave forte)
SESSION_SECRET=sua_chave_secreta_muito_longa_e_segura_aqui_com_pelo_menos_32_caracteres